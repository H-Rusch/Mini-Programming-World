<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane fx:id="dialogPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
              fx:controller="controller.PresentsDialogController">
    <content>
        <HBox>
            <Label fx:id="presentsLabel" text="Geschenke: "/>
            <TextField fx:id="presentsInput"/>
        </HBox>
    </content>

    <ButtonType fx:id="submitButton" fx:constant="OK"/>
    <ButtonType fx:id="cancelButton" fx:constant="CANCEL"/>
</DialogPane>
